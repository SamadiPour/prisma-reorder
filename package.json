{
  "name": "prisma-reorder",
  "version": "1.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "prisma-reorder": "./dist/cli.js"
  },
  "files": [
    "dist/"
  ],
  "keywords": [
    "prisma",
    "mysql",
    "mariadb",
    "database",
    "migration",
    "column-order",
    "schema",
    "orm",
    "typescript",
    "cli"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SamadiPour/prisma-reorder.git"
  },
  "bugs": {
    "url": "https://github.com/SamadiPour/prisma-reorder/issues"
  },
  "homepage": "https://github.com/SamadiPour/prisma-reorder/prisma-reorder#readme",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "prepublishOnly": "npm run test && npm run build",
    "prepack": "npm run build",
    "start": "ts-node src/index.ts",
    "dev": "ts-node-dev --respawn src/index.ts",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "release:patch": "npm version patch && npm publish",
    "release:minor": "npm version minor && npm publish",
    "release:major": "npm version major && npm publish",
    "release:beta": "npm version prerelease --preid=beta && npm publish --tag beta"
  },
  "author": "Amir Hossein SamadiPour",
  "license": "MIT",
  "description": "A TypeScript NPM package that enhances Prisma ORM by enabling automatic column reordering in MySQL/MariaDB databases to match the column order defined in the Prisma schema.",
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^18.19.111",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.7.0",
    "prettier": "^2.8.8",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@mrleebo/prisma-ast": "^0.12.1",
    "commander": "^14.0.0",
    "dotenv": "^16.5.0",
    "dotenv-expand": "^12.0.2",
    "mysql2": "^3.14.1"
  }
}
